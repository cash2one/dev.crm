INSERT INTO `doctor_profile` (`id`, `doctor_id`, `qualification`, `description`, `education`, `work_experience`, `work_start_year`, `lang`, `award`, `publication`, `website_url`, `date_created`, `date_updated`, `date_deleted`) VALUES (1,1,0,'主任医师、科主任、安徽医科大学教授、中华医学会安徽省耳鼻咽喉头颈外科专业委员会常委、安徽省康复医学会耳鼻咽喉科专业委员会常委、安庆市耳鼻咽喉科分会主任委员、安庆市中西医结合耳鼻咽喉科协会副理事长兼秘书长、泛太平洋地区整形外科协会会员、《中国中西医结合耳鼻咽喉科杂志》编委',NULL,NULL,1992,NULL,'安庆市率先开展了鼓室成形+听小骨植入手术、耳硬化症的手术、鼻内镜下垂体瘤切除术、喉部激光手术、下咽癌切除+胸大肌皮瓣修复术、颈部气管造口成形术等特大、新手术，居省内先进水平。在国家级刊物上发表了论文十','于1999年参加首届全国大型医疗器械上岗考试并获得CDFI上岗证书。从事超声工作十多年，在《中国超声诊断杂志》、《皖南医学院学报》等国家级核心刊物上发表学术论文17篇。',NULL,NULL,NULL,NULL),(2,2,0,NULL,NULL,NULL,1995,NULL,NULL,NULL,NULL,NULL,NULL,NULL),(3,3,0,NULL,NULL,NULL,2000,NULL,NULL,NULL,NULL,NULL,NULL,NULL),(4,4,0,NULL,NULL,NULL,1988,NULL,NULL,NULL,NULL,NULL,NULL,NULL),(5,5,0,NULL,NULL,NULL,1992,NULL,'发表论文30余篇，先后获得安徽省优秀科技论文一等奖1篇、安庆市优秀科技论文一等奖1篇，二等奖2篇，三等奖1篇。获得安庆市科学技术二等奖2项，三等奖1项，领衔中华预防医学会安庆片区科研项目1项。先后入编',NULL,NULL,NULL,NULL,NULL),(6,6,0,'1996年率先开展妇癌的手术和化疗及更年期妇女激素替代治疗；1999年参加安徽省卫生厅援外专家医疗队，赴也门工作两年并受该国省卫生厅的表彰；2006年率先开展晚期卵巢癌卷地毯式子宫切除+卵巢肿瘤切除+盆腹腔淋巴结清扫术；2007年成功治疗一例五胞胎孕妇',NULL,NULL,1984,NULL,'在CN杂志上发表论文10余篇，获安庆市科技进步二等奖一项、三等奖一项',NULL,NULL,NULL,NULL,NULL),(7,7,0,'主任医师,中国医师协会呼吸医师分会全国基层委员会委员，中华医学会安徽省呼吸内科分会常务委员、安徽省结核病学分会委员、安徽省抗癌协会肺癌专业委员会皖西南分会主任委员','1984年毕业于安徽医学院医学系','长期从事临床工作，能熟练诊治呼吸系统的常见病、多发病、少见病及疑难病，能独立完成专科重大会诊及抢救。在抢救危重症呼吸衰竭、肺心病等病人方面积累丰富的临床经验，并率先在我市开展有创、无创序贯呼吸支持抢救危重症呼吸衰竭，先后主持开展胸膜穿刺活检、经皮肺穿刺活检、透视下支气管镜肺活检、支气管镜下肺泡灌洗、支气管镜下气道内球囊扩张、气道内支架置入、气道内氩气刀治疗等呼吸介入技术。',1994,NULL,NULL,NULL,NULL,NULL,NULL,NULL),(8,8,0,'中国医师协会肾脏病分会委员，中华医学会安徽省肾脏病分会常务委员，安徽省医院协会血液净化管理专业委员会常务委员，安徽医科大学兼职副教授，安庆市内科学会委员','1989年毕业于皖南医学院医疗系临床医学专业','从事内科、肾内科临床工作20年，具有丰富的临床理论与实践知识，主治各种原发性和继发性肾小球疾病、肾间质及肾血管疾病，在本地区率先开展腹膜透析、肾穿刺、颈内静脉永久性置管技术，获安庆市科技进步奖一项（B超定位下单人操作肾活检技术），擅长各种血液净化技术及临时性和永久性血管通路的建立及制作，其中钛轮钉制作动静脉内瘘技术处省内领先地位；先后荣获医院2009年度、2012年度优秀科主任称号',1988,NULL,'安庆市科技进步三等奖一项','学术论文10余篇',NULL,NULL,NULL,NULL),(9,9,0,'肾内科主任医师、科副主任、安医大兼职教授','1973年毕业于安徽医科大学医疗系',' 长期从事内科临床工作30余年，有着丰富的临床理论知识与实践经验。',1983,NULL,'获市政府科学技术进步奖一项','发表医学论文11篇，其中国家级论文5篇，省级刊物论文6篇',NULL,NULL,NULL,NULL),(10,10,0,NULL,'徐州医学院、安徽省立医院和天津眼科医院进修学习','专业从事眼前节疾病的Y-G激光治疗和儿童斜、弱视的诊治.',1990,NULL,NULL,'在各级医刊上发表论文15篇',NULL,NULL,NULL,NULL),(12,11,0,'副教授，硕士生导师。广西医科大学第一附属医院心电诊断科科主任，广西医学会心电学专科分会副主任委员','1986年9月至1992年7月在华西医科大学攻读六年制本科临床医学专业，获医学学士学位；2004年9月至2007年7月在广西医科大学就读全日制硕士研究生内科学专业，获医学硕士学位。','1992年8月大学本科毕业至今一直在广西医科大学第一附属医院心电诊断科从事临床、教学及科研工作。',1992,NULL,'曾获广西科技进步三等奖1项；曾被评为“2011-2012学年度广西医科大学最受欢迎的老师”。',NULL,NULL,NULL,NULL,NULL),(13,12,0,'广西医科大学一附院男性学科（男科和性学科）主任，教授，主任医师，广西医科大学内科学教授、硕士生导师。中华医学会男科学分会全国委员，广西男科学会主任委员，中华医学会广西分会理事，广西性学会副会长，大学生性健康教育中心首席顾问。广西青年科技奖获得者，广西“新世纪十百千人才工程”入选人物','1979-1984 广州中山医科大学医学系（大学本科），1998-2001 日本国立东京大学农学和生命科学研究生院攻读博士学位','广西医科大学第一附院内分泌科、内科教研室，广西医科大学第一附院',1984,NULL,'科研获广西科技进步二等奖和广西医药科技进步二等奖。是广西男性学科的带头人和广西男科学会主要创始人，在国内外男性学科领域有一定学术影响',NULL,NULL,NULL,NULL,NULL),(14,13,0,NULL,NULL,NULL,1992,NULL,NULL,NULL,NULL,NULL,NULL,NULL),(15,14,0,NULL,NULL,NULL,1992,NULL,NULL,NULL,NULL,NULL,NULL,NULL),(16,15,0,'介入治疗科主任、放射科主任、教授、主任医师、硕士生导师。肿瘤介入治疗专家，影像学专家，放射学诊断和介入治疗的学科带头人。广西政协委员，广西抗癌协会常务理事、中国抗癌协会肿瘤介入诊疗专业委员会广西区委员、介入诊疗专业委员会主任委员。《中国介入影像与治疗学》杂志编委。肿瘤医学院诊断学教研室主任。',NULL,NULL,1992,NULL,'主持和主要参加6项科研课题的研究，获2000年广西医药卫生科技进步二等奖一项及2001年广西科技进步三等奖一项。共撰写专业论文20篇，2篇获广西自然科学学术论文二等奖。',NULL,NULL,NULL,NULL,NULL),(17,16,0,'胸瘤外科副主任、副教授、副主任医师、硕士生导师，胸外科专家','1984年毕业于广西医科大学医疗系本科        2000年赴德国进修留学一年 ','毕业后一直在附属肿瘤医院从事肿瘤胸外科工作。1990年晋升为主治医师，1996年晋升为副主任医师。2002年4月起任肿瘤胸外科副主任。',1984,NULL,'曾留学德国，主攻肺癌基因方面的研究，发明国家专利两项。承担自治区科技厅自然基金科研项目一项，卫生厅自筹资金科研项目一项。参加科研项目二项。获国家专利二项（本专业）。','发表“组胺及其受体与原发性肺癌”，“VATS在肺癌肺叶切除中的应用”，“VATS下食管癌切除二例报道”，“保留迷走神经干的贲门食管癌根治术”，“肺癌的基因治疗”等相关学术论文十余篇。',NULL,NULL,NULL,NULL),(18,17,0,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL),(19,18,0,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL),(20,19,0,NULL,'1982~1987 江苏徐州医学院1993~1996 第二军医大学 硕士2010~2012 第二军医大学 博士','病毒性肝炎及疑难病的治疗',1993,NULL,NULL,'在国内外杂志发表论文20余篇，参与编写多部书籍，如《桑福德抗微生物治疗指南》、《内科学理论与技术新进展》、《The Sanford Guide To Antimicrobial Therapy（第33',NULL,NULL,NULL,NULL),(21,20,0,'成功开展多项新技术、新疗法。负责多项国际多中心新药临床验证。任上海市全科医师及上海市住院医师规范化培训考核出题专家和考官，曾被评为第二军医大学校A级教员及住院医师规范化培训优秀教师','1990~1995 第二军医大学 本科1998~2001 第二军医大学 硕士2003年 德国弗莱格 实体肿瘤的细胞免疫治疗2004~2007 第二军医大学 博士','1995~2012年 上海长征医院 内科   2013~至今 上海东方肝胆外科医院 生物治疗科',1995,NULL,'以第1完成人获军队医疗成果奖三等奖1项，第2完成人获上海市科技进步二等奖和军队医疗成果奖二等奖各1项，其他还获得海市科技进步一等奖、中华医学科技奖二等奖、上海医学科技二等奖各1项。','共发表学术论文60多篇，其中第1作者发表SCI论文4篇，参与13部专著编写',NULL,NULL,NULL,NULL),(22,21,0,'','1996~2003 第二军医大学7年制本硕连读学员2008~2011 第二军医大学，博士生','2003~2005 东方肝胆外科医院 住院医师2005~2013 东方肝胆外科医院 主治医师2013~至今 东方肝胆外科医院 副主任医师',2007,NULL,'获军队医疗成果三等奖1项，获国家专利授权5项','获各类基金6项，其中2010年获得长三角联合攻关基金资助。发表论文30余篇，其中第一作者及通讯作者发表SCI论文7篇、总影响因子12.103。参编专著2部',NULL,NULL,NULL,NULL),(23,23,0,'复旦大学附属华山医院外科教授，主任医师，博士生导师，医学博士。分别于2011年在美国Cleveland Clinic，2012年在美国Duke University Hospital从事访问学者工作。主要从事肝胆外科疾病的诊治，擅长腹腔镜手术。对原发性肝癌、转移性肝癌、肝脏良性肿瘤、胆囊及胆管肿瘤、胆囊结石及肝内外胆管结石、门静脉高压脾功能亢进等疾病具有丰富的诊治经验，其中腹腔镜肝切除术每年约10',NULL,NULL,NULL,NULL,'担任国家自然科学基金评审专家，上海市医学会外科青年学组委员，中西医结合学会上海市外科委员，Saudi Journal of Gastroenterology和Organic Chemistry杂志的审','研究工作主要包括肝胆疾病的基础及临床。负责国家自然科学基金2项，复旦大学课题1项，参与国家自然科学基金3项，上海市卫计委课题2项，浦东新区人才计划1项。第一完成人获2011年上海市中西医科学技术奖2等',NULL,NULL,NULL,NULL),(24,24,0,'任中国医师协会外科医师分会结直肠外科医师委员会委员，中国抗癌协会大肠癌专业委员会腹腔镜学组委员兼秘书，中国抗癌协会大肠癌专业委员会青年委员，海峡两岸医药卫生交流协会肿瘤防治专家委员会胃肿瘤专业学组委员，上海市医学会外科分会微创外科学组秘书，《中华结直肠疾病电子杂志》通讯编委。',NULL,NULL,NULL,NULL,NULL,'独立承担国家自然科学基金项目、上海市卫生局重点项目与上海市科委重点项目，并入选上海交通大学晨星计划。获教育部科技进步一等奖，上海市科技进步一等奖，上海市医学科技奖一等奖以及中华医学奖二等奖（第三完成人',NULL,NULL,NULL,NULL),(25,25,0,NULL,'1980－1990上海医科大学医学系。1990至今复旦大学附属肿瘤医院头颈外科。',NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL);
