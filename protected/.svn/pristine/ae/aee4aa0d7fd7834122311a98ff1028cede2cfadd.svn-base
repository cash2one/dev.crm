CREATE TABLE `region_city` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `country_name` varchar(45) DEFAULT NULL,
  `country_code` char(3) DEFAULT NULL,
  `state_name` varchar(45) DEFAULT NULL,
  `state_id` int(11) DEFAULT NULL,
  `code` varchar(20) DEFAULT NULL,
  `name` varchar(45) DEFAULT NULL,
  `name_cn` varchar(45) DEFAULT NULL,
  `description` varchar(255) DEFAULT NULL,
  `display_order` int(11) DEFAULT NULL,
  `image_url` varchar(255) DEFAULT NULL,
  `tn_url` varchar(255) DEFAULT NULL,
  `date_created` datetime DEFAULT NULL,
  `date_updated` datetime DEFAULT NULL,
  `date_deleted` datetime DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `country_city` (`code`,`country_code`),
  KEY `country_code_idx` (`country_code`),
  KEY `cityState_idx` (`state_id`),
  CONSTRAINT `cityState` FOREIGN KEY (`state_id`) REFERENCES `region_state` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB AUTO_INCREMENT=390 DEFAULT CHARSET=utf8;
